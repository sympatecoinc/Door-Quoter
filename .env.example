DATABASE_URL="postgresql://postgres:password@localhost:5432/door_quoter"

# QuickBooks Online API Credentials
# Get these from https://developer.intuit.com/app/developer/dashboard
QUICKBOOKS_CLIENT_ID="your_client_id_here"
QUICKBOOKS_CLIENT_SECRET="your_client_secret_here"
QUICKBOOKS_ENVIRONMENT="sandbox"  # or "production"
QUICKBOOKS_REDIRECT_URI="http://localhost:3000/api/quickbooks/callback"

# QuickBooks Webhook Verification Token
# Get this from the QuickBooks Developer Dashboard when creating a webhook subscription
# This is used to verify that webhook requests are actually from QuickBooks
QUICKBOOKS_WEBHOOK_VERIFIER_TOKEN="your_webhook_verifier_token_here"
